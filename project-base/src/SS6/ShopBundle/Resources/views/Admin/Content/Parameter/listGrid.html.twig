{% extends '@SS6Shop/Admin/Grid/Grid.html.twig' %}

{% block grid_no_data %}
	{{ 'Nebyly nalezeny žádné parametry. Nejprve musíte nějaké vytvořit.'|trans }}
{% endblock %}

{% block grid_pager_totalcount %}
	{% set entityName = 'parametry'|trans %}
	{{ parent() }}
{% endblock %}

{% block grid_inline_edit_add_button %}
	{% set addEntity = 'Vytvořit parametr'|trans %}
	{{ parent() }}
{% endblock %}

{% block grid_value_cell_id_visible %}
	{% if row.p.visible %}
		{{ 'Ano'|trans }}
	{% else %}
		{{ 'Ne'|trans }}
	{% endif %}
{% endblock %}