{
    "name": "shopsys/product-feed-heureka-delivery",
    "type": "library",
    "description": "Shopsys Framework plugin interface for Heureka delivery XML product feed generation",
    "keywords": ["Heureka", "product delivery feed", "XML feed", "plugin", "Shopsys Framework", "SSFW"],
    "license": "MIT",
    "authors": [
        {
            "name": "Shopsys",
            "homepage": "https://www.shopsys.com/"
        }
    ],
    "autoload": {
        "psr-4": {
            "Shopsys\\ProductFeed\\HeurekaDeliveryBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\ProductFeed\\HeurekaDeliveryBundle\\": "tests/"
        }
    },
    "require": {
        "php": "^7.1",
        "shopsys/doctrine-orm": "^2.6.2",
        "shopsys/form-types-bundle": "8.0.x-dev",
        "shopsys/framework": "8.0.x-dev",
        "shopsys/migrations": "8.0.x-dev",
        "shopsys/plugin-interface": "8.0.x-dev",
        "symfony/config": "^3.4",
        "symfony/dependency-injection": "^3.4",
        "symfony/framework-bundle": "^3.4",
        "symfony/http-kernel": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0|^7.0",
        "shopsys/coding-standards": "8.0.x-dev",
        "twig/extensions": "^1.5.1",
        "twig/twig": "^2.4.8"
    },
    "conflict": {
        "symfony/dependency-injection": "3.4.15|3.4.16",
        "twig/twig": "2.6.1"
    }
}
