{
    "name": "shopsys/form-types-bundle",
    "type": "library",
    "description": "Shopsys Framework custom Symfony form types",
    "keywords": ["form", "form type", "Shopsys Framework", "SSFW"],
    "license": "MIT",
    "authors": [
        {
            "name": "Shopsys",
            "homepage": "https://www.shopsys.com/"
        }
    ],
    "autoload": {
        "psr-4": {
            "Shopsys\\FormTypesBundle\\": "src/"
        }
    },
    "require": {
        "php": "^7.2",
        "symfony/config": "^3.4",
        "symfony/dependency-injection": "^3.4",
        "symfony/form": "^3.4",
        "symfony/framework-bundle": "^3.4",
        "symfony/http-kernel": "^3.4",
        "symfony/options-resolver": "^3.4",
        "symfony/translation": "^3.4"
    },
    "require-dev": {
        "shopsys/coding-standards": "8.0.0"
    },
    "conflict": {
        "symfony/dependency-injection": "3.4.15|3.4.16"
    }
}
